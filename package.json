{
  "name": "@continuous-auth/client",
  "version": "1.0.3",
  "description": "Safely enable 2FA on your CI published packages",
  "main": "lib/module/index.js",
  "author": "Samuel Attard",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "rm -rf public_out && cross-env NODE_ENV=production webpack --config webpack.production.config.js",
    "build:server": "rm -rf lib && tsc",
    "prettier:write": "prettier --write \"src/**/*.{ts,tsx}\"",
    "start": "npm run start:prod",
    "start:dev:server": "cross-env DEBUG=cfa* REDIS_URL=redis://localhost NO_DB_SSL=true DATABASE_URL=postgresql://cfa-user:cfa-pass@localhost:5433/cfa node lib/server",
    "start:prod": "cross-env DEBUG=cfa* node lib/server",
    "test": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "watch:dev:client": "node --max-old-space-size=4096 node_modules/.bin/webpack-dev-server --progress --profile --colors --open",
    "watch:dev:server": "nodemon --watch src/server --exec \"tsc && npm run start:dev:server\" -e ts"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/connect-redis": "^0.0.10",
    "@types/debug": "^4.1.4",
    "@types/dotenv-safe": "^5.0.3",
    "@types/enzyme": "^3.10.3",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.13",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.15",
    "@types/joi": "^14.3.3",
    "@types/morgan": "^1.7.36",
    "@types/node": "^12.6.8",
    "@types/passport": "^1.0.0",
    "@types/passport-github": "^1.1.5",
    "@types/pg-promise": "^5.4.3",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "@types/webpack-env": "^1.14.0",
    "autoprefixer": "^9.6.1",
    "cache-loader": "^4.1.0",
    "core-js": "^3.1.4",
    "csp-html-webpack-plugin": "^3.0.2",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "evergreen-ui": "^4.18.1",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-express": "^1.10.1",
    "jest-junit": "^8.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hooks-async": "^3.0.1",
    "react-hot-loader": "^4.12.8",
    "react-router-dom": "^5.0.1",
    "sass-loader": "^7.1.0",
    "sqlite3": "^4.1.0",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-subresource-integrity": "^1.3.2"
  },
  "dependencies": {
    "@octokit/rest": "^16.28.5",
    "@slack/bolt": "^1.2.0",
    "compression": "^1.7.4",
    "connect-redis": "^3.4.2",
    "cross-env": "^5.2.0",
    "debug": "^4.1.1",
    "dotenv-safe": "^6.1.0",
    "express-history-api-fallback": "^2.2.1",
    "express-session": "^1.16.2",
    "joi": "^14.3.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "pg": "^7.11.0",
